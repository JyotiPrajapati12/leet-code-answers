class Solution {
public:
    int minOperations(vector<string>& logs) {
        int n=logs.size();
        int dis=0;
        for(int i=0;i<n;i++){
            if(logs[i]=="../"){
                if(dis)
                dis--;
            }else if(logs[i]=="./"){
                
            }
            else{
           dis++;
            }
        }
        return dis;
    }
};
