class Solution {
public:
    int lengthOfLastWord(string s) {
        int count=0;
        for(int i=s.length();i>=0;i--)
        if(s.back()==' ')
            s.pop_back();
        for(int i=s.length()-1;i>=0;i--){
             if(s[i]!=' ')
             count++;
             else
            break;
        }  
      return count;
    }   
};
